{
  "name": "LegionGuard",
  "description": "Bot",
  "version": "0.0.1",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js",
    "postinstall": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run build; fi",
    "build": "NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --colors -p",
    "serve-dev": "NODE_ENV=development node internals/webpack/server.dev.js",
    "serve-prod": "NODE_ENV=production node internals/webpack/server.prod.js",
    "pagespeed": "./internals/scripts/pagespeed",
    "setup": "NODE_ENV=initialisation ./internals/scripts/init",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:js:staged": "eslint-staged",
    "lint:css": "stylelint ./app/**/*.css",
    "lint:css:staged": "stylelint-staged",
    "lint:staged": "npm run lint:js:staged && npm run lint:css:staged"
    },
    "dependencies": {
      "discord.js": "11.1.0",
      "fs": "0.0.1-security",
      "unique-random": "2.0.0",
      "moment": "2.22.1",
      "memejs": "1.0.7",
      "ms": "^2.1.1",
      "superagent": "^3.8.3",
      "snekfetch": "^4.0.4",
      "gif-search": "^2.0.1",
      "moment-duration-format": "2.2.2",
      "request": "2.81.0",
      "yt-search": "^0.1.4",
      "ytdl": "^0.10.1",
      "node-opus": "^0.3.0"
      }
     }
